/* 水平间距 */
/* 水平间距 */
.title {
  width: 100%;
  height: 40px;
  line-height: 40px;
  display: flex;
  margin-bottom: 5px;
  position: -webkit-sticky;
  position: sticky;
  top: 0rpx;
  z-index: 99;
  background-color: #eee;
}
.title text {
  font-weight: 900;
  margin-left: 15px;
  margin-right: 15px;
}
.title input {
  background-color: #eaeffa;
  width: 220px;
  height: 40px;
  border-radius: 20px;
  padding-left: 19px;
  line-height: 40px;
}
swiper image {
  width: 100%;
}
.nav_ul {
  width: 100%;
  height: 260rpx;
  display: flex;
  margin-top: 30px;
  flex-wrap: wrap;
  align-items: center;
  align-content: space-around;
  padding-left: 15px;
}
.nav_ul ._li {
  flex: 20%;
}
.p {
  width: 100%;
  text-align: center;
}
.list {
  width: 100%;
}
.list ._ul {
  width: 100%;
  height: 500px;
  display: flex;
  flex-wrap: wrap;
}
.list ._ul image {
  width: 100%;
}
.list ._ul ._li {
  width: 46%;
  border: 1px solid #ccc;
  margin: 5px;
  border-radius: 8px;
}

